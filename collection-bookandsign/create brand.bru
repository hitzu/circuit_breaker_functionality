meta {
  name: create brand
  type: http
  seq: 1
}

post {
  url: {{base_url}}/brands
  body: json
  auth: inherit
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "key": "Lusso Recepciones",
    "name": "Lusso Recepciones",
    "theme": {
      "primaryColor": "#000000",
      "secondaryColor": "#ffffff",
      "logo": "https://example.com/logo.png"
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
